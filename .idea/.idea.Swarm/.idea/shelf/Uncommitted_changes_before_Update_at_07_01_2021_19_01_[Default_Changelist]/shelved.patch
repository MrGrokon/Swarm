+        Vector3 GetGridNormalVector(Brush brush)
+        {
+            switch(brush.settings.gridPlane)
+            {
+            case GridPlane.XY:
+                return new Vector3(0, 0, 1);
+            case GridPlane.XZ:
+                return new Vector3(0, 1, 0);
