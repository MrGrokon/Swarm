+
+        public bool HasMultipleSelectedBrushes()
+        {
+            int selectedCount = 0;
+            for (int i = 0; i < brushes.Count; i++)
+            {
+                if (brushes[i].selected)
+                {
+                    selectedCount++;
+
+                    if (selectedCount > 1)
+                        return true;
+                }
+            }
+            return false;
+        }
+
+        public bool HasSelectedBrushes()
+        {
+            for (int i = 0; i < brushes.Count; i++)
+            {
+                if (brushes[i].selected)
+                    return true;
+            }
+            return false;
+        }
+
+        public int GetFirstSelectedBrushIndex()
+        {
+            for (int i = 0; i < brushes.Count; i++)
+            {
+                if (brushes[i].selected)
+                    return i;
+            }
