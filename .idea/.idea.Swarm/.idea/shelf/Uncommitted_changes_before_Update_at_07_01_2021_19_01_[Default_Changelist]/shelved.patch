+        static Rect TransformRect(Rect rect, float xOffset, float yOffset, float xScale, float yScale)
+        {
+            return new Rect(rect.x + rect.width * xOffset, rect.y + rect.height * yOffset, rect.width * xScale, rect.height * yScale);
+        }
