+		{
+			Undo.RegisterCompleteObjectUndo (PrefabPainterSettings.current, "PP: Arrange Brushes By Name");
+
+			brushes.Sort (delegate(Brush x, Brush y)
+			{
+				return EditorUtility.NaturalCompare(x.name, y.name);
+			});
+		}
+	}
